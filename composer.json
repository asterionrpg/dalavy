{
    "name": "asterion/dalavy",
    "license": "proprietary",
    "type": "page",
    "description": "Geographical module of Asterion RPG",
    "keywords": [
        "Asterion",
        "DÃ¡lavy",
        "geographic",
        "RPG",
        "module"
    ],
    "autoload": {
        "psr-4": {
            "Asterion\\": "Asterion/"
        }
    },
    "require": {
        "php": ">=7.3",
        "drdplus/rules-skeleton": "^3.0"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "granam/exceptions-hierarchy": "^5.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "drdplus/rules-skeleton": true,
            "granam/web-content-builder": true
        }
    },
    "scripts": {
        "pre-autoload-dump": [
            "cd tests && rm -f RulesSkeleton && ln -s ../vendor/drdplus/rules-skeleton/tests/RulesSkeleton ."
        ]
    }
}
